{
    "extends": [ "eslint:recommended" ],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "arrowFunctions": true,
        "blockBindings": true,
        "classes": true,
        "defaultParams": true,
        "destructuring": true,
        "forOf": true,
        "generators": false,
        "modules": true,
        "objectLiteralComputedProperties": true,
        "objectLiteralDuplicateProperties": false,
        "objectLiteralShorthandMethods": true,
        "objectLiteralShorthandProperties": true,
        "spread": true,
        "superInFunctions": true,
        "templateStrings": true
      }
    },
    "rules": {
      "no-console": "off"
    },
    "globals": {
        "env": true,
        "rm": true,
        "TimelineMax": true,
        "Sine": true,
        "it": true,
        "expect": true
    }
  }
  